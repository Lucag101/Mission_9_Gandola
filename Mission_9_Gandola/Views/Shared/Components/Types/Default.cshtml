@{
}
@model IEnumerable<string>
<div class="d-grid gap-2">
    <a asp-controller="Home" asp-action="Index" asp-route-category=""
       asp-route-pageNum="1" class="btn btn-outline-secondary p-2 rounded-circle btn-sm" style="background-color: lightgray">Home</a>

    @foreach (string t in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-category="@t" asp-route-pageNum="1"
           class="btn @(ViewBag.SelectedType == t ? "btn-primary" : "btn-outline-secondary")">@t</a>

    }
</div>
